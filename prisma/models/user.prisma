model User {
  id         Int      @id @default(autoincrement())
  username   String   @unique
  name       String
  surname    String
  avatar     String
  lastSeenAt DateTime @default(now())
  password   String
  email      String   @unique
  contactOf  Contact[] @relation(name:"ContactUser")
  contacts   Contact[] @relation(name: "ContactOwner")
}
